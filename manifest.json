{
    "name": "Moodle Plus",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Moodleにちょっとした機能を追加します!",
    "content_scripts": [
        {
            "matches": [
                "https://lms.omu.ac.jp/*",
                "https://moodle.s.kyushu-u.ac.jp/*"
            ],
            "js": [
                "./dist/content_script.js"
            ]
        }
    ],
    "icons": {
        "16": "./icons/icon16.png",
        "32": "./icons/icon32.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png"
    }
}